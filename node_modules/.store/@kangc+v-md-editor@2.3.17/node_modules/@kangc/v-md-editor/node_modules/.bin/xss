#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/easybbs-front-web/easybbs-front-web/node_modules/.store/xss@1.0.14/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/easybbs-front-web/easybbs-front-web/node_modules/.store/xss@1.0.14/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../xss@1.0.14/node_modules/xss/bin/xss" "$@"
else
  exec node  "$basedir/../../../../../../xss@1.0.14/node_modules/xss/bin/xss" "$@"
fi
