#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/easybbs-front-web/easybbs-front-web/node_modules/.store/d3-dsv@1.2.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/easybbs-front-web/easybbs-front-web/node_modules/.store/d3-dsv@1.2.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../d3-dsv@1.2.0/node_modules/d3-dsv/bin/dsv2json" "$@"
else
  exec node  "$basedir/../../../../../d3-dsv@1.2.0/node_modules/d3-dsv/bin/dsv2json" "$@"
fi
