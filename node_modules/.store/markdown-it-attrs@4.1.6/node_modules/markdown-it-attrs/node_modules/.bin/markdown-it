#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="$basedir/E:/easybbs-front-web/easybbs-front-web/node_modules/.store/markdown-it@12.3.2/node_modules"
else
  export NODE_PATH="$NODE_PATH:$basedir/E:/easybbs-front-web/easybbs-front-web/node_modules/.store/markdown-it@12.3.2/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../markdown-it@12.3.2/node_modules/markdown-it/bin/markdown-it.js" "$@"
else
  exec node  "$basedir/../../../../../markdown-it@12.3.2/node_modules/markdown-it/bin/markdown-it.js" "$@"
fi
